#!/usr/bin/make -f
export DH_VERBOSE=1

%:
	dh $@

override_dh_auto_configure:
	dh_auto_configure -- ${JOBSARGS} --enable-avahi --enable-zlib

override_dh_auto_build:
	make ${JARGS}

override_dh_strip:
	dh_strip --dbg-package=tvheadend-dbg

override_dh_auto_install:
	dh_auto_install --destdir=debian/tvheadend

